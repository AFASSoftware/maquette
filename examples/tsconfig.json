{
  "compilerOptions": {
    "lib": [
      "dom",
      "es2015",
      "es6"
    ],
    "types": [],
    "module": "es6",
    "noImplicitAny": true,
    "preserveConstEnums": true,
    "sourceMap": true,
    "outDir": "../build/js",
    "rootDir": "..",
    "target": "es5",
    "noEmitOnError": true,
    "strictNullChecks": true,
    "noImplicitThis": true
  },
  "include": [
    "./**/*.ts",
    "../typings/**/*.d.ts",
    "./**/*.d.ts"
  ],
  "exclude": []
}
