{
  "name": "maquette",
  "description": "Minimalistic Virtual DOM implementation with support for animated transitions.",
  "homepage": "http://maquettejs.org/",
  "keywords": [
    "virtual",
    "dom",
    "animation",
    "transitions"
  ],
  "version": "2.5.4-pre.1",
  "author": "Johan Gorter <johan@johangorter.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/AFASSoftware/maquette"
  },
  "main": "./dist/maquette.js",
  "typings": "./dist/maquette.d.ts",
  "scripts": {
    "test-also-with-browser": "gulp test && cd examples/todomvc && bower install && cd ../../browser-tests && npm install && node sauce.js",
    "test": "cross-env TS_NODE_FAST=true mocha --compilers ts:ts-node/register --recursive",
    "coverage": "cross-env TS_NODE_FAST=true nyc --check-coverage -- mocha --compilers ts:ts-node/register --reporter dot --recursive"
  },
  "license": "MIT",
  "tonicExampleFilename": "examples/tonic-example.js",
  "nyc": {
    "include": [
      "src/**/*.ts",
      "typings/**/*.ts"
    ],
    "extension": [
      ".ts"
    ],
    "reporter": [
      "lcov",
      "html",
      "text-summary"
    ],
    "cache": true,
    "temp-directory": "./build/nyc/cache",
    "all": true,
    "check-coverage": true,
    "report-dir": "./build/coverage",
    "es-module": false,
    "lines": 100,
    "statements": 100,
    "functions": 100,
    "branches": 100,
    "watermarks": {
      "lines": [
        75,
        100
      ],
      "functions": [
        75,
        100
      ],
      "branches": [
        75,
        100
      ],
      "statements": [
        75,
        100
      ]
    }
  },
  "devDependencies": {
    "@types/node": "8.0.17",
    "Set": "0.4.1",
    "browser-sync": "2.18.13",
    "chai": "4.1.0",
    "chai-as-promised": "7.1.1",
    "cross-env": "5.0.1",
    "debug": "2.6.8",
    "del": "3.0.0",
    "gulp": "3.9.1",
    "gulp-bump": "2.7.0",
    "gulp-filter": "5.0.0",
    "gulp-git": "2.4.1",
    "gulp-istanbul": "1.1.2",
    "gulp-mocha": "4.3.1",
    "gulp-plumber": "1.1.0",
    "gulp-prompt": "0.2.0",
    "gulp-rename": "1.2.2",
    "gulp-shell": "0.6.3",
    "gulp-sourcemaps": "2.6.0",
    "gulp-tag-version": "1.3.0",
    "gulp-typedoc": "2.0.2",
    "gulp-typescript": "3.2.1",
    "gulp-uglify": "3.0.0",
    "gulp-wrap-js": "0.4.1",
    "ink-docstrap": "1.3.0",
    "inquirer": "3.2.1",
    "istanbul": "0.4.5",
    "jsdoc": "3.5.3",
    "jsdom": "11.1.0",
    "jsdom-global": "3.0.2",
    "mocha": "3.5.0",
    "mocha-jsdom": "1.1.0",
    "nyc": "11.1.0",
    "remap-istanbul": "0.9.5",
    "sinon": "2.4.1",
    "sinon-chai": "2.12.0",
    "ts-node": "3.3.0",
    "tsd": "0.6.5",
    "tslint": "5.5.0",
    "typedoc": "0.8.0",
    "typescript": "2.4.2"
  }
}
